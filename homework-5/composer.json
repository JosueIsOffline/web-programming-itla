{
  "name": "josue/api-platform",
  "require": {
    "arkham/framework": "*",
    "vlucas/phpdotenv": "^5.6"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../framework"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "serve": "php -S localhost:8000 -t public/"
  }
}
